{
  "name": "support-i18next",
  "displayName": "support-i18next",
  "description": "show i18next translation in the editor",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MtBlue81/vscode-extension-support-i18next"
  },
  "license": "MIT",
  "scripts": {
    "prepackage": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "typescript": "^5.1.6"
  }
}
